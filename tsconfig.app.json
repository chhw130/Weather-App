{
  "compilerOptions": {
    "types": ["webpack-env", "node"],
    "composite": true,
    "target": "esnext",
    "module": "esnext",
    "moduleResolution": "Node",
    "baseUrl": ".",

    "jsx": "preserve",
    "paths": {
      "@/*": ["src/*"]
    }
  },
  "exclude": ["node_modules", "dist"],
  "include": ["src/**/*.ts", "src/**/*.d.ts", "src/**/*.tsx", "src/**/*.vue"]
}
